<?xml version="1.0" encoding="UTF-8"?>
<SolverDefinition	xmlns="www.iric.net/SolverDefinition/1.0"
					name="RRI"
					caption="Rainfall-Runoff-Inundation v1.4.2.2 64 bit"
					version="1.4.2.2"
					copyright="Takahiro Sayama , RiverLINK Co., Ltd. ,  Mitsui Consultants Co., Ltd."
					release="2018.1.1"
					homepage="http://i-ric.org"
					executable="rri.exe"
					iterationtype="time"
					gridtype="structured2d">

<CalculationCondition>

  <Tab name="Main" caption="条件">
    <Item name="rrifile" caption="RRI Input File" >
      <Definition valueType="filename" default="Select File" />
    </Item>
  </Tab>
  
  
	<!--<Tab name="FileControl" caption="地形条件">
		--><!-- RRI_Input  L1 バージョン情報--><!--
		<Item name="RRI_version" caption="RRI Version【L1】" >
				--><!-- RRI_version →RRIソースでは、format_version--><!--
			<Definition valueType="string" default="RRI Input Format Ver1.4.2" />
		</Item>
		
		--><!-- RRI_Input  L2 = blank --><!--
		
		--><!-- RRI_Input  L4	DEMファイルのパス（地形データ） --><!--
		<Item name="demfile" caption="DEMファイル" >
			<Definition valueType="filename" default="Select File" />
		</Item>
		
		--><!-- RRI_Input  L6	dirファイルのパス（流下方向データ） --><!--
		<Item name="dirfile" caption="DIRファイル" >
			<Definition valueType="filename" default="Select File" />
		</Item>
		
		--><!-- RRI_Input  L5	accファイルのパス（累積流量データ） --><!--
		<Item name="accfile" caption="ACCファイル" >
			<Definition valueType="filename" default="Select File" />
		</Item>

   
		--><!-- RRI_Input  L7	（Null） --><!--
	</Tab>
  
  <Tab name="rain" caption="降雨条件">
    --><!-- RRI_Input  L3 Rainファイルのパス（雨量データ） --><!--
    <Item name="rainfile" caption="ファイル指定" >
      <Definition valueType="filename" default="Select File" />
    </Item>

    --><!-- RRI_Input  L14	雨量データの南西座標値（経度）lon --><!--
    <Item name="xllcorner_rain" caption="雨量データの南西座標値（経度）lon" >
      <Definition valueType="real" default="110.200000" />
    </Item>

    --><!-- RRI_Input  L15	雨量データの南西座標値（緯度）lat --><!--
    <Item name="yllcorner_rain" caption="雨量データの南西座標値（緯度）lat" >
      <Definition valueType="real" default="-8.300000" />
    </Item>

    --><!-- RRI_Input  L16	雨量データの空間解像度（経度␣緯度の順） --><!--
    <Item name="cellsize_rain_x"  caption="雨量データの空間解像度 経度">
      <Definition valueType="real" default="0.0083333333" />
    </Item>
    <Item name="cellsize_rain_y" caption="雨量データの空間解像度 緯度">
      <Definition valueType="real" default="0.0083333333" />
    </Item>
  </Tab>

  <Tab name="evaporation" caption="蒸発散">
    <Item name="evp_switch" caption="蒸発散の考慮フラグ">
      <Definition valueType="integer" default="0">
        <Enumeration value="0" caption="設定しない" />
        <Enumeration value="1" caption="設定する" />
        <Enumeration value="2" caption="設定する（GAから無）" />
      </Definition>
    </Item>

    <Item name="evpfile" caption="ファイル指定" >
      <Definition valueType="filename" default="Select File" >
      <Dependency>
        <Condition type="or">
          <Condition type="isEqual" target="evp_switch" value="1" />
          <Condition type="isEqual" target="evp_switch" value="2" />
        </Condition>
        <Action type="enable" />
      </Dependency>
     </Definition>
    </Item>

    --><!-- RRI_Input  L14	雨量データの南西座標値（経度）lon --><!--
    <Item name="xllcorner_evp" caption="蒸発散データの南西座標値（経度）lon" >
      <Definition valueType="real" default="110.200000" >
        <Dependency>
          <Condition type="or">
            <Condition type="isEqual" target="evp_switch" value="1" />
            <Condition type="isEqual" target="evp_switch" value="2" />
          </Condition>
          <Action type="enable" />
        </Dependency>
      </Definition>
    </Item>

    --><!-- RRI_Input  L15	雨量データの南西座標値（緯度）lat --><!--
    <Item name="yllcorner_evp" caption="蒸発散データの南西座標値（緯度）lat" >
      <Definition valueType="real" default="-8.300000" >
        <Dependency>
        <Condition type="or">
          <Condition type="isEqual" target="evp_switch" value="1" />
          <Condition type="isEqual" target="evp_switch" value="2" />
        </Condition>
        <Action type="enable" />
      </Dependency>
     </Definition>
    </Item>

    --><!-- RRI_Input  L16	雨量データの空間解像度（経度␣緯度の順） --><!--
    <Item name="cellsize_evp_x"  caption="蒸発散データの空間解像度 経度">
      <Definition valueType="real" default="0.0083333333" >
        <Dependency>
          <Condition type="or">
            <Condition type="isEqual" target="evp_switch" value="1" />
            <Condition type="isEqual" target="evp_switch" value="2" />
          </Condition>
          <Action type="enable" />
        </Dependency>
      </Definition>
    </Item>
    <Item name="cellsize_evp_y" caption="蒸発散データの空間解像度 緯度">
      <Definition valueType="real" default="0.0083333333">
        <Dependency>
          <Condition type="or">
            <Condition type="isEqual" target="evp_switch" value="1" />
            <Condition type="isEqual" target="evp_switch" value="2" />
          </Condition>
          <Action type="enable" />
        </Dependency>
      </Definition>
    </Item>

  </Tab>
  

	<Tab name="Basic_Condition" caption="基本条件">
		--><!-- RRI_Input  L8	地形や雨量データの座標指定方法 --><!--
		<Item name="utm" caption="座標系                        【L8】" >
			<Definition valueType="integer" default="0">
				<Enumeration caption="緯度経度" value="0" />
				<Enumeration caption="UTM" value="1" />
			</Definition>
		</Item>

		--><!-- RRI_Input  L9	二次元モデルの移動方向 --><!--
		<Item name="eight_dir" caption="移動方向                     【L9】" >
			<Definition valueType="integer" default="1">
				<Enumeration value="0" caption="4方向" />
				<Enumeration value="1" caption="8方向" />
			</Definition>
		</Item>

		--><!-- RRI_Input  L10	シミュレーション期間 --><!--
		<Item name="lasth" caption="シミュレーション期間(hr)     【L10】" >
			<Definition valueType="integer" default="24" max="20000" />
		</Item>
		
		--><!-- RRI_Input  L11	氾濫原のシミュレーション時間ステップ --><!--
		<Item name="dt" caption="氾濫原計算時間ステップ(s)【L11】" >
			<Definition valueType="integer" default="600" max="20000" />
		</Item>
		
		--><!-- RRI_Input  L12	河道内のシミュレーション時間ステップ --><!--
		<Item name="dt_riv" caption="河道計算時間ステップ(s)   【L12】" >
			<Definition valueType="integer" default="60" max="20000" />
		</Item>
		
		--><!-- RRI_Input  L13	出力ファイル数 --><!--
		<Item name="outnum" caption="出力ファイル数（=シミュレーション期間/出力時間）【L13】" >
			<Definition valueType="integer" default="24" max="100000" />
		</Item>
	</Tab>-->

  
  
	<!--<Tab name="Rain_Evap" caption="降雨・蒸発散⇒iRIC内設定が望ましい" >
		<GridLayout>
			<Label row="0" col="1" caption="       降雨【L3,14～16】" />
			<Label row="0" col="2" caption="       蒸発散【L74～78】" />
	
			<Label row="1" col="0" caption="設定の有無                            【L74】" />
			<Label row="2" col="0" caption="ファイル                            【L3】【L75】" />
			<Label row="3" col="0" caption="南西座標値［経度(度)/X(m)］【L14】【L76】" />
			<Label row="4" col="0" caption="南西座標値［緯度(度)/Y(m)］【L15】【L77】" />
			<Label row="5" col="0" caption="空間解像度［経度(度)/X(m)］【L16】【L78】" />
			<Label row="6" col="0" caption="空間解像度［緯度(度)/X(m)］【L16】【L78】" />

			<Label row="1" col="1" caption="設定する（必須）" />

			--><!-- RRI_Input  L3 Rainファイルのパス（雨量データ） --><!--
			<Item row="2" col="1" name="rainfile" >
				<Definition valueType="filename" default="Select File" />
			</Item>--><!--

			

			--><!-- RRI_Input  L17	（Null） --><!--

			--><!-- RRI_Input  L74	蒸発散の与え方（設定しない(0) or 設定する(1) or GAなし(2)） --><!--
			<Item row="1" col="2" name="evp_switch" >
				<Definition valueType="integer" default="0">
					<Enumeration value="0" caption="設定しない" />
					<Enumeration value="1" caption="設定する" />
					<Enumeration value="2" caption="設定する（GAから無）" />
				</Definition>
			</Item>
		
			--><!-- RRI_Input  L75	蒸発散設定ファイルのパス --><!--
			<Item row="2" col="2" name="evpfile" >
				<Definition valueType="filename" default="Select File" >
					<Dependency>
						<Condition type="or">
							<Condition type="isEqual" target="evp_switch" value="1" />
							<Condition type="isEqual" target="evp_switch" value="2" />
						</Condition>
						<Action type="enable" />
					</Dependency>
				</Definition>
			</Item>
		
			--><!-- RRI_Input  L76	蒸発散データの南西座標値（経度） --><!--
			<Item row="3" col="2" name="xllcorner_evp" >
				<Definition valueType="real" default="135.0" >
					<Dependency>
						<Condition type="or">
							<Condition type="isEqual" target="evp_switch" value="1" />
							<Condition type="isEqual" target="evp_switch" value="2" />
						</Condition>
						<Action type="enable" />
					</Dependency>
				</Definition>
			</Item>
		
			--><!-- RRI_Input  L77	蒸発散データの南西座標値（緯度） --><!--
			<Item row="4" col="2" name="yllcorner_evp" >
				<Definition valueType="real" default="34.65" >
					<Dependency>
						<Condition type="or">
							<Condition type="isEqual" target="evp_switch" value="1" />
							<Condition type="isEqual" target="evp_switch" value="2" />
						</Condition>
						<Action type="enable" />
					</Dependency>
				</Definition>
			</Item>
		
			--><!-- RRI_Input  L78	蒸発散データの空間解像度（経度␣緯度の順） --><!--
			<Item row="5" col="2" name="cellsize_evp_x" >
				<Definition valueType="real" default="0.0102062072" >
					<Dependency>
						<Condition type="or">
							<Condition type="isEqual" target="evp_switch" value="1" />
							<Condition type="isEqual" target="evp_switch" value="2" />
						</Condition>
						<Action type="enable" />
					</Dependency>
				</Definition>
			</Item>
			<Item row="6" col="2" name="cellsize_evp_y" >
				<Definition valueType="real" default="0.0102062072" >
					<Dependency>
						<Condition type="or">
							<Condition type="isEqual" target="evp_switch" value="1" />
							<Condition type="isEqual" target="evp_switch" value="2" />
						</Condition>
						<Action type="enable" />
					</Dependency>
				</Definition>
			</Item>
		
			--><!-- RRI_Input  L79	（Null） --><!--
		</GridLayout>
	</Tab>-->

	<!--<Tab name="River_Condition" caption="河道条件">
		<GroupBox>
			--><!-- RRI_Input  L18	河道の粗度係数（マニング） --><!--
			<Item name="ns_river" caption="河道粗度（m^(-1/3)s）【L18】" >
				<Definition valueType="real" default="0.030" />
			</Item>
		</GroupBox>

		--><!-- RRI_Input  L80	河道延長の与え方（設定しない(0) or 設定する(1)） --><!--
		<GroupBox>
			<Item name="sec_length_switch" caption="河道延長の任意設定【L80】">
				<Definition valueType="integer" default="0">
					<Enumeration value="0" caption="設定しない" />
					<Enumeration value="1" caption="設定する（地理情報で設定）" />
				</Definition>
			</Item>
		</GroupBox>

		<GroupBox caption="河道断面">
			<GroupBox caption="矩形断面設定">
				--><!-- RRI_Input  L46	河道形状の与え方(自動(0) or File指定(1)） --><!--
				<Item name="rivfile_switch" caption="  河道断面の与え方【L46】" >
					<Definition valueType="integer" default="0">
						<Enumeration value="0" caption="河道形状係数より" />
						<Enumeration value="1" caption="個別に設定（地理情報で設定）" />
					</Definition>
				</Item>
	
				<GroupBox caption="河道形状係数設定" >
					--><!-- RRI_Input  L38	河道判定の閾値（地点上流メッシュ数） --><!--
					<GroupBox>
						<Item name="riv_thresh" caption="河道判定の閾値（地点上流メッシュ数）【L38】">
							<Definition valueType="integer" default="20" >
								<Dependency>
									<Condition type="isEqual" target="rivfile_switch" value="0" />
									<Action type="enable" />
								</Dependency>
							</Definition>
						</Item>
					</GroupBox>

					<GroupBox caption="形状変数（=C×A＾S）" >
					<GridLayout>
						<Label row="0" col="1" caption="                                     C" />
						<Label row="0" col="2" caption="                                     S" />
	
						<Label row="1" col="0" caption="河道幅  【L39-40】" />
						<Label row="2" col="0" caption="河道深さ【L41-42】" />
				
						--><!-- RRI_Input  L39	河道幅算定のための形状変数C --><!--
						<Item row="1" col="1" name="width_param_c" caption="河道幅形状変数Cw（width=Cw×A＾Sw）【L39】" >
							<Definition valueType="real" default="5.5" >
								<Dependency>
									<Condition type="isEqual" target="rivfile_switch" value="0" />
									<Action type="enable" />
								</Dependency>
							</Definition>
						</Item>

						--><!-- RRI_Input  L40	河道幅算定のための形状変数S --><!--
						<Item row="1" col="2" name="width_param_s" caption="河道幅形状変数Sw（width=Cw×A＾Sw）【L40】" >
							<Definition valueType="real" default="0.61" >
								<Dependency>
									<Condition type="isEqual" target="rivfile_switch" value="0" />
									<Action type="enable" />
								</Dependency>
							</Definition>
						</Item>

						--><!-- RRI_Input  L41	河道深さ算定のための形状変数C --><!--	
						<Item row="2" col="1" name="depth_param_c" caption="河道深さ形状変数Cd（depth=Cd×A＾Sd）【L41】" >
							<Definition valueType="real" default="0.94" >
								<Dependency>
									<Condition type="isEqual" target="rivfile_switch" value="0" />
									<Action type="enable" />
								</Dependency>
							</Definition>
						</Item>

						--><!-- RRI_Input  L42	河道深さ算定のための形状変数S --><!--
						<Item row="2" col="2" name="depth_param_s" caption="河道深さ形状変数Sd（depth=Cd×A＾Sd）【L42】" >
							<Definition valueType="real" default="0.34" >
								<Dependency>
									<Condition type="isEqual" target="rivfile_switch" value="0" />
									<Action type="enable" />
								</Dependency>
							</Definition>
						</Item>
					</GridLayout>
					</GroupBox>

					<GroupBox caption="堤防" >
						--><!-- RRI_Input  L43	堤防の高さ（地盤高からの高さ） --><!--
						<Item name="height_param" caption="堤防高【L43】" >
							<Definition valueType="real" default="0.00" >
								<Dependency>
									<Condition type="isEqual" target="rivfile_switch" value="0" />
									<Action type="enable" />
								</Dependency>
							</Definition>
						</Item>

						--><!-- RRI_Input  L44	堤防位置の閾値（地点上流メッシュ数） --><!--
						<Item name="height_limit_param" caption="堤防位置の閾値（地点上流メッシュ数）【L44】" >
							<Definition valueType="real" default="1000" >
								<Dependency>
									<Condition type="isEqual" target="rivfile_switch" value="0" />
									<Action type="enable" />
								</Dependency>
							</Definition>
						</Item>
					</GroupBox>
				</GroupBox>

					--><!-- RRI_Input  L45	（Null） --><!--

			</GroupBox>
			<GroupBox caption="任意断面設定⇒iRIC内設定が望ましい">
				--><!-- RRI_Input  L83	河道断面の与え方（設定しない(0) or 設定する(1)） --><!--
				<Item name="sec_switch" caption="任意断面の与え方          【L83】">
					<Definition valueType="integer" default="0">
						<Enumeration value="0" caption="設定しない" />
						<Enumeration value="1" caption="設定する" />
					</Definition>
				</Item>

				--><!-- RRI_Input  L84	河道断面の設定位置ファイルのパス --><!--
				<Item name="sec_map_file" caption="河道断面設定位置ファイル【L84】" >
					<Definition valueType="filename" default="Select File" >
						<Dependency>
							<Condition type="isEqual" target="sec_switch" value="1" />
							<Action type="enable" />
						</Dependency>
					</Definition>
				</Item>
		
				--><!-- RRI_Input  L85	断面毎の断面特性ファイルのパス --><!--
				<Item name="sec_file" caption="断面特性ファイル            【L85】" >
					<Definition valueType="string" default=".\Riv\sec_" >
						<Dependency>
							<Condition type="isEqual" target="sec_switch" value="1" />
							<Action type="enable" />
						</Dependency>
					</Definition>
				</Item>
				
				--><!-- RRI_Input  L86	（Null） --><!--

			</GroupBox>
		</GroupBox>
		
	</Tab>-->

	<!--<Tab name="Initial_Condition" caption="初期水深(ﾎｯﾄｽﾀｰﾄ用)⇒iRIC内処理が望ましい">
		<GridLayout>
			<Label row="0" col="1" caption="                           【L51】" />
			<Label row="0" col="2" caption="ファイル" />
	
			<Label row="1" col="0" caption="斜面        【L52】" />
			<Label row="2" col="0" caption="河道        【L53】" />
			<Label row="3" col="0" caption="基岩        【L54】" />
			<Label row="4" col="0" caption="GreenAmpt【L55】" />

			--><!-- RRI_Input  L51	斜面の初期水深の与え方 --><!--
			<Item row="1" col="1" name="init_slo_switch" >
				<Definition valueType="integer" default="0">
					<Enumeration value="0" caption="設定しない" />
					<Enumeration value="1" caption="設定する" />
				</Definition>
			</Item>
			--><!-- RRI_Input  L51 河道の初期水深の与え方 --><!--
			<Item row="2" col="1" name="init_riv_switch" >
				<Definition valueType="integer" default="0">
					<Enumeration value="0" caption="設定しない" />
					<Enumeration value="1" caption="設定する" />
				</Definition>
			</Item>
			--><!-- RRI_Input  L51 基岩の初期水深の与え方 --><!--
			<Item row="3" col="1" name="init_gw_switch" >
				<Definition valueType="integer" default="0">
					<Enumeration value="0" caption="設定しない" />
					<Enumeration value="1" caption="設定する" />
				</Definition>
			</Item>
			--><!-- RRI_Input  L51 GreenAmptの初期水深の与え方 --><!--
			<Item row="4" col="1" name="init_gampt_ff_switch" >
				<Definition valueType="integer" default="0">
					<Enumeration value="0" caption="設定しない" />
					<Enumeration value="1" caption="設定する" />
				</Definition>
			</Item>

			--><!-- RRI_Input  L52	斜面における初期水深設定ファイルのパス　※単位：[m] --><!--
			<Item row="1" col="2" name="initfile_slo" >
				<Definition valueType="filename" default="Select File" >
					<Dependency>
						<Condition type="isEqual" target="init_slo_switch" value="1" />
						<Action type="enable" />
					</Dependency>
				</Definition>
			</Item>

			--><!-- RRI_Input  L53	河道における初期水深設定ファイルのパス　※単位：[m] --><!--
			<Item row="2" col="2" name="initfile_riv" >
				<Definition valueType="filename" default="Select File" >
					<Dependency>
						<Condition type="isEqual" target="init_riv_switch" value="1" />
						<Action type="enable" />
					</Dependency>
				</Definition>
			</Item>

			--><!-- RRI_Input  L54	基岩における初期水深設定ファイルのパス　※単位：[m] --><!--
			<Item row="3" col="2" name="initfile_gw" >
				<Definition valueType="filename" default="Select File" >
					<Dependency>
						<Condition type="isEqual" target="init_gw_switch" value="1" />
						<Action type="enable" />
					</Dependency>
				</Definition>
			</Item>

			--><!-- RRI_Input  L55	Green-Amptにおける初期水深設定ファイルのパス　※単位：[m] --><!--
			<Item row="4" col="2" name="initfile_gampt_ff" >
